<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.gocrazy">

    <uses-permission android:name="android.permission.EXPAND_STATUS_BAR"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/go_crazy"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Sign_In"></activity>
        <activity android:name=".Forgot_Password"></activity>
        <activity android:name=".Activate_Account"></activity>
        <activity android:name=".Main_frame"></activity>
        <activity android:name=".FullImageActivity"></activity>
        <activity android:name=".MyActivity"></activity>
    </application>

</manifest>